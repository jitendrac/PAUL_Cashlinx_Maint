  <data name="Query_1_Desc" xml:space="preserve">
    <value>Find a jewelry item that has an inactive refurb tag on it with no ICN.</value>
  </data>
  <data name="Query_1_Name" xml:space="preserve">
    <value>FindJewelryItemWithInactiveRefurbAndNoIcn</value>
  </data>
  <data name="Query_1_Params" xml:space="preserve">
    <value>Query1Params</value>
  </data>
  <data name="Query_1_SQL" xml:space="preserve">
    <value>Query1SQL</value>
  </data>
  <data name="Query_2_Desc" xml:space="preserve">
    <value>Find an ICN based on the refurb number.</value>
  </data>
  <data name="Query_2_Name" xml:space="preserve">
    <value>FindIcnBasedOnRefurbNumber</value>
  </data>
  <data name="Query_2_Params" xml:space="preserve">
    <value>p_rfb_no:number,p_store_number:string</value>
  </data>
  <data name="Query_2_SQL" xml:space="preserve">
    <value>select  icn_store, icn_year, icn_doc, icn_doc_type, icn_item, icn_sub_item, status_cd, md_desc from mdse where rfb_no = ?p_rfb_no? and storenumber = ?p_store_number?</value>
  </data>
  <data name="Query_3_Desc" xml:space="preserve">
    <value>Find active general inventory by manufacturer, model, and serial number.</value>
  </data>
  <data name="Query_3_Name" xml:space="preserve">
    <value>FindGeneralInventoryByMfgrModelSerialNumber</value>
  </data>
  <data name="Query_3_Params" xml:space="preserve">
    <value>p_manufacturer:string,p_model_nr:string,p_serial_nr:string,p_store_number:string</value>
  </data>
  <data name="Query_3_SQL" xml:space="preserve">
    <value>select icn_store, icn_year, icn_doc, icn_doc_type, icn_item, icn_sub_item, status_cd, md_desc from mdse where storenumber = ?p_store_number? and (?p_manufacturer? is null or manufacturer = upper(?p_manufacturer?)) and (?p_model_nr? is null or model = upper(?p_model_nr?)) and (?p_serial_nr? is null or serial_number = upper(?p_serial_nr?))</value>
  </data>
  <data name="Query_4_Desc" xml:space="preserve">
    <value>Find active jewelry inventory by category, karat, and weight.</value>
  </data>
  <data name="Query_4_Name" xml:space="preserve">
    <value>FindJewelryInventoryByCategoryKaratWeight</value>
  </data>
  <data name="Query_4_Params" xml:space="preserve">
    <value>p_cat_code:number,p_weight:string,p_karat:string,p_store_number:string</value>
  </data>
  <data name="Query_4_SQL" xml:space="preserve">
    <value>select /*+ INDEX(mdse,mdse(md_desc)) */ icn_store, icn_year, icn_doc, icn_doc_type, icn_item, icn_sub_item, status_cd, md_desc from mdse where storenumber = ?p_store_number? and cat_code between 1000 and 1999 and (?p_cat_code? is null or cat_code = ?p_cat_code?) and (?p_weight? is null or md_desc like '%' || ?p_weight? || '% GRM%') and (?p_karat? is null or md_desc like '%' || ?p_karat? || '% KT%')</value>
